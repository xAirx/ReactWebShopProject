{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["wesbos"],
  "env": { "browser": true, "es6": true, "jest": true },
  "rules": {
    /*  "semi": ["error", "never"], */
    "quotes": [2, "single"],
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        /*  "trailingComma": "es5", */
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
